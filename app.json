{
  "expo": {
    "name": "eventapp",
    "slug": "eventapp",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash1.png",
      "resizeMode": "cover"
    },
    "android": {
      "package": "com.TopEventsinJamaica",
      "permissions" : [
        "ACCESS_FINE_LOCATION",
        "READ_EXTERNAL_STORAGE",
        "MANAGE_DOCUMENTS",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier":"com.TopEventsinJamaica",
      "supportsTablet": true
    }
  }
}